{
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.3",
        "vlucas/phpdotenv": "^5.2",
        "php-di/php-di": "^6.3",
        "php-di/slim-bridge": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "tests": "vendor/bin/phpunit -c tests/phpunit.xml --no-coverage"
    }
}